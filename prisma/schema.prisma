model data_kelas {
  id_kelas        String @id
  kelas           String
  id_walikelas    String
  nama_walikelas  String
  nik_walikelas   String
  id_guru_bk      String
  nama_guru_bk    String
  nik_guru_bk     String
}

model data_riwayat_perubahan {
  no          Int @id @default(autoincrement())
  waktu       DateTime    @default(now())
  status      String
  data        String
  detail      String

}
